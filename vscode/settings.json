{
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "editor.tabSize": 2,
    
    // Path to directory to save notes.
    "vsnotes.defaultNotePath": "/Users/mentalray/Documents/vsnotes/",
    "window.zoomLevel": 1,
    "gitlens.keymap": "alternate",
    "markdown.preview.scrollEditorWithPreview": false,
    "markdown.preview.scrollPreviewWithEditor": false,
    "gitlens.historyExplorer.enabled": true,
    "[typescript]": {},
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "typescript": "javascriptreact",
    },
    "files.associations": {
        "*.css": ["postcss", "css"],
    },
    "gitlens.resultsExplorer.files.layout": "auto",
    "workbench.sideBar.location": "right",
    "vim.disableExtension": true,
    "editor.renderWhitespace": "boundary",
    "typescript.tsserver.log": "verbose",
"vim.statusBarColorControl": true,
"workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
},
"workbench.panel.defaultLocation": "right",
"gitlens.views.fileHistory.enabled": true,
"gitlens.views.lineHistory.enabled": true,
"gitlens.views.compare.files.layout": "auto",
"gitlens.views.search.files.layout": "auto",
"extensions.autoUpdate": false,
}